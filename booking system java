    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton7ActionPerformed
        JOptionPane.showMessageDialog(null,"Booked sucessful. You will be called Shortly by our agents");
        jTabbedPane1.setSelectedIndex(5);
        try
        {
            
            String url="jdbc:mysql://localhost:3306/hotel";
            connection=DriverManager.getConnection(url,"root","root");
            stmt=connection.createStatement();
        String fname, lname,email, telno,dob,pass,username,time,capacity,event,date,month,year;
        fname=jTextField3.getText();
        lname=jTextField4.getText();
        email=jTextField5.getText();
        telno=jTextField6.getText();
        dob=jTextField7.getText();
        pass=jTextField3.getText();
        username=jTextField14.getText();
        time=insertTime.getText();
        capacity=jTextField17.getText();
        event=insertEvent.getText();
        date=theDay.getName();
        month=theMonth.getName();
        year=theYear.getName();
        
        
        String Query="INSERT INTO sign(fname,lname,userName,email,tel,dob,pass,time,capacity,event,date,month,year) VALUES ('"+fname+"','"+lname+"','"+email+"','"+telno+"','"+dob+"','"+pass+"','"+username+"','"+time+"','"+capacity+"','"+event+"','"+date+"','"+month+"','"+year+"')";
        stmt.executeUpdate(Query);
        }catch(SQLException ex){
            JOptionPane.showMessageDialog(null, ex.toString());
            jTextField3.setText(null);
            jTextField4.setText(null);
            jTextField5.setText(null);
            jTextField6.setText(null);
            jTextField7.setText(null);
            jTextField8.setText(null);
            jTextField14.setText(null);
            jTextField17.setText(null);
            insertTime.setText(null);
            insertEvent.setText(null);
            String day = null;
            theDay.setName(day);
            String month = null;
            theMonth.setName(month);
            String year = null;
            theYear.setName(year);
        }
        
       
    }//GEN-LAST:event_jButton7ActionPerformed

