    public void login()
    {
        try
        {
            
            String url="jdbc:mysql://localhost:3306/hotel";
            connection=DriverManager.getConnection(url,"root","root");
            stmt=connection.createStatement();
        String userNameInsert=jTextField1.getText();
        String passWordInsert=jTextField2.getText();
        String query="SELECT * FROM `sign` WHERE userName='"+userNameInsert+"' AND pass ='"+passWordInsert+"'";
        ResultSet rs;
        rs=stmt.executeQuery(query);
        int count=0;
        while(rs.next())
        {
            count++;
            if(count==0)
            {
              JOptionPane.showMessageDialog(null,"USER NOT FOUND");  
            }
            else if(count==1)
            {
                JOptionPane.showMessageDialog(null, "LOGIN SUCESSFUL");
                jTabbedPane1.setSelectedIndex(2);
            }
            else
            {
                JOptionPane.showMessageDialog(null, "Check User and Pass And Try Again");
            }
            
        }
        
        }catch(SQLException ex){
            JOptionPane.showMessageDialog(null, ex.toString());
            
        }
        
        
    }

